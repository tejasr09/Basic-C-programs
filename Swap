#include <stdio.h>

int main() {
int first, second, temp;
    printf("Enter the first number:");
        scanf("%d", &first);
    printf("Enter the second number:");
        scanf("%d", &second);
    
    temp=first;
    first=second;
    second=temp;
    
    printf("The first number after swapping in %d\n", first);
    printf("The second number after swapping in %d\n", second);
    return 0;
}
