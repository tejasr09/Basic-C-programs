#include <stdio.h>

int main() {
    char c;
    printf("Enter the letter to be identified:");
    scanf("%c", &c);
        if(c=='a' || c=='e' || c=='i' || c=='o' || c=='u' || c=='A' || c=='E' || c=='I' || c=='I' || c=='U')
        
             printf("It is a vowel");
        else
            printf("It is a consonant");
        
        
        return 0;
}
