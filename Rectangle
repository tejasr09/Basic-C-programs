#include <stdio.h>

int main() {
    int l, b, peri, area;
        printf("Enter the length and breadthh of the rectangle:");
            scanf("%d %d", &l, &b);
            
            area=l*b;
            peri=2*(l+b);
            
        printf("The area of the rectangle is %d\n", area);
        printf("The perimeter of the rectangle is %d\n", peri);
        
    if(area>peri)
    {
        printf("The area is more than the perimeter.\n");
    }
    else
        printf("The perimeter is more than the area.");
    return 0;
}
