#include <stdio.h>

int main() {
    float x1, x2, x3, y1, y2, y3, s1, s2;
        printf("Enter the coordinates of the 3 points:");
            scanf("%f %f %f %f %f %f", &x1, &y1, &x2, &y2, &x3, &y3);
            
        s1=(y2-y1)/(x2-x1);
        s2=(y3-y2)/(x3-x2);
        
        printf("The given coordinates are (%f, %f), (%f, %f) and (%f, %f)\n", x1,y1, x2, y2, x3, y3);
        
        if(s1==s2)
        {
            printf("The points are collinear.");
        }
        else if(s1!=s2)
        {
            printf("The points are not collinear.");
        }
    return 0;
}
