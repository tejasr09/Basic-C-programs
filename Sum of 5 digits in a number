#include <stdio.h>

int main() {
int num, rem, sum=0;
    printf("Enter a 5-digit number:");
        scanf("%d", &num);
        
    if(num<10000 || num>99999)
    {
        printf("Error:Enter a valid 5-digit number\n");
    }
    else
    {
    for(int i=0; i<5; i++)
        {
            rem=num%10;
            sum=sum+rem;
            num=num/10;
        }
            printf("The sum of the 5-digit number is %d", sum);
    }
return 0;
}
